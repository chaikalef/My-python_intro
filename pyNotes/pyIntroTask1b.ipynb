   "source": [
    "inNum = int(input(\"Enter number: \"))\n",
    "\n",
    "def factorial(num):\n",
    "    res = 1\n",
    "    \n",
    "    if num >= 2:\n",
    "        for counter in range(2, num + 1):\n",
    "            res *= counter\n",
    "    elif num < 0:\n",
    "        res = 'ValueError'\n",
    "    \n",
    "    return res\n",
    "\n",
    "print(str(inNum) + \"! = \" + str(factorial(inNum)))"
   ]
  }
